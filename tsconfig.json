{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ],
      "@/screens/*": [
        "./screens/*"
      ],
      "@/components/*": [
        "./components/*"
      ],
      "@/hooks/*": [
        "./hooks/*"
      ],
      "@/services/*": [
        "./services/*"
      ],
      "@/theme/*": [
        "./theme/*"
      ],
      "@/constants/*": [
        "./constants/*"
      ],
      "@/navigation/*": [
        "./navigation/*"
      ],
      "@/assets/*": [
        "./assets/*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "nativewind-env.d.ts"
  ]
}
